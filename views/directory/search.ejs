<a href="/directory">Go Back to Directory</a>
<h2>Search</h2>
<form action="/directory/search/files" method="GET">
  <label>Name</label>
  <input type="text" name="name" value="<%=searchOptions.name%>" />
  <button type="submit">Search</button>
</form>
<br />
<% folders.forEach(folder => { %>
<div><%=folder.foldername%></div>
<% }) %>
<br />
<% files.forEach(file => { %>
<div><%=file.filename%></div>
<% }) %>
<br />
